
@model Proyecto_LaboratorioJARAF.Models.CITAS

@{
    ViewBag.Title = "Edit";
}

<h2 id="textot">Actualizar cita</h2>
<body style=" margin:2cm" id="fondop">
   
    <img src="~/Content/Estilo CSS HTML/perfildeusuario.jpg" id="cabezoncito3">

    @using (Html.BeginForm("Edit", "Cita", FormMethod.Post))
     {
        <p id="textot">id cita</p>
        @Html.TextBoxFor(a => a.IdCita)
        <p id="textot">id examen</p>
        @Html.TextBoxFor(a => a.IdExamenes)
        <br />
        <p id="textot">Fecha</p>
        @Html.TextBoxFor(a => a.Fecha)
        <br />
        <p id="textot">Hora</p>
        @Html.TextBoxFor(a => a.Hora)
        <br><br>
        <input type="submit" />
     }
</body>